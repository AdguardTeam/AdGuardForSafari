{
  "name": "adguard-mini",
  "version": "2.0.0.rc",
  "description": "Part of AdGuard Mini swift app responsible for displaying of data for user",
  "author": "AdGuard Team",
  "license": "GPL-3.0-or-later",
  "nodemonConfig": {
    "watch": [
      "build/*"
    ],
    "delay": 1500,
    "ext": "html,css,js,svg",
    "events": {
      "quit": "osascript -e \"tell application \\\"Adguard Mini\\\" to quit\""
    }
  },
  "scripts": {
    "start": "webpack watch --config ./AdguardMini/sciter-ui/scripts/webpack/webpack.config.dev.js",
    "build:userRules": "webpack --config ./AdguardMini/sciter-ui/scripts/webpack/webpack.inline.js",
    "build:dev": "node ./AdguardMini/sciter-ui/scripts/prebuildFrontendDeps.js && webpack --config ./AdguardMini/sciter-ui/scripts/webpack/webpack.config.dev.js",
    "build:prod": "node ./AdguardMini/sciter-ui/scripts/prebuildFrontendDeps.js && webpack --config ./AdguardMini/sciter-ui/scripts/webpack/webpack.config.prod.js",
    "devserver": "rimraf build && webpack server --config ./AdguardMini/sciter-ui/scripts/webpack/webpack.config.devserver.js --env webBuild",
    "lint:fix": "yarn lint --fix",
    "prepare": "husky install",
    "lint": "eslint -c ./AdguardMini/sciter-ui/scripts/lint/prod.mjs --ext .tsx,.ts ./AdguardMini/sciter-ui/",
    "locales:exist": "sciter-infra-utils locales-exist ./AdguardMini/sciter-ui/modules/common/intl/index.ts",
    "locales:check": "sciter-infra-utils locales-check ./AdguardMini/sciter-ui",
    "locales:checkInhouse": "sciter-infra-utils locales-check ./AdguardMini/sciter-ui --inhouse-only --no-check-unused-keys --no-check-files-existence",
    "locales:pull": "sciter-infra-utils locales-pull",
    "locales:pushMaster": "sciter-infra-utils locales-push",
    "theme:generate": "sciter-infra-utils generate-stylesheets ./AdguardMini/sciter-ui/modules/common/theme/default ADG",
    "watchProject": "nodemon --exec ./AdguardMini/Scripts/rebuildDebugBuild.sh"
  },
  "dependencies": {
    "@adg/sciter-utils-kit": "^0.6.4",
    "@adguard/rules-editor": "^1.3.0",
    "classix": "^2.1.32",
    "date-fns": "^4.1.0",
    "is-valid-domain": "^0.1.6",
    "lodash": "^4.17.21",
    "mobx": "^6.9.0",
    "mobx-react-lite": "^3.4.3",
    "preact": "^10.15.1",
    "qs": "^6.11.2",
    "uuid": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.2"
  },
  "devDependencies": {
    "@adg/proto-generator": "^1.7.1",
    "@adg/sciter-infra-utils": "^0.13.9",
    "@types/google-protobuf": "^3.15.5",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.17.6",
    "@types/qs": "^6.9.8",
    "@types/uuid": "^9.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "5.2.7",
    "expose-loader": "^4.1.0",
    "file-loader": "^6.2.0",
    "google-protobuf": "^3.21.2",
    "html-inline-script-webpack-plugin": "^3.2.1",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^2.0.22",
    "postcss-loader": "^7.3.2",
    "postcss-nested": "^6.0.1",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.3",
    "typescript": "^5.0.4",
    "webpack": "^5.84.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^4.15.0",
    "@stylistic/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^8.37.0",
    "lint-staged": "^13.2.1",
    "eslint": "^9.31.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^51.4.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-mobx": "^0.0.13",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "typescript-eslint": "^8.37.0"
  }
}
